{
  "name": "playbook-partnership",
  "version": "1.0.0",
  "description": "A repository for the playbook partnership collaboration.",
  "private": true,
  "workspaces": [
    "./app",
    "./components/**"
  ],
  "scripts": {
    "build:sql": "mkdir -p db/migrations && ts-node -r tsconfig-paths/register cli/build-sql.ts > db/migrations/00_init.sql",
    "build": "npx --workspace=app next build",
    "codegen:components": "ts-node cli/sync-component-import.ts",
    "codegen:icons": "python cli/convert-icons.py icons/services/src icons/services/index.ts",
    "codegen:requirements": "ts-node cli/enumerate-requirements.ts",
    "dev": "npx --workspace=app next dev",
    "start:dispatcher": "ts-node -r tsconfig-paths/register cli/resolve-process-dispatcher.ts",
    "start:worker": "ts-node -r tsconfig-paths/register cli/resolve-process-worker.ts",
    "start": "npx --workspace=app next start"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.11.6",
    "@blueprintjs/icons": "^4.10.0",
    "@blueprintjs/select": "^4.8.6",
    "@blueprintjs/table": "^4.7.9",
    "@mdi/js": "^7.0.96",
    "fast-sha256": "^1.3.0",
    "glob": "^8.0.3",
    "levelup": "^5.1.1",
    "next": "^12.3.1",
    "pg": "^8.8.0",
    "pg-boss": "^8.3.0",
    "pg-listen": "^1.7.0",
    "pg-query-stream": "^4.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "sqldown": "^2.1.0",
    "subleveldown": "^6.0.1",
    "swr": "^1.3.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/levelup": "^5.1.2",
    "@types/memdown": "^3.0.1",
    "@types/node": "^18.8.4",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.21",
    "@types/subleveldown": "^4.1.1",
    "memdown": "^6.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
